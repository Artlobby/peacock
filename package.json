{
	"name": "peacock",
	"version": "2.0.41",
	"description": "Open source privacy-first browser that hosts your data on the blockchain.",
	"main": "main.js",
	"repository": "https://github.com/Codiscite/peacock/",
	"keywords": [
		"browser",
		"web",
		"web3",
		"blockchain",
		"privacy"
	],
	"build": {
		"appId": "com.codiscite.peacock",
		"win": {
			"target": "nsis",
			"icon": "images/Peacock2.0.ico"
		},
		"mac": {
			"category": "books",
			"target": [
				"dmg"
			]
		},
		"nsis": {
			"perMachine": true
		}
	},
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"release": "electron-builder --publish=always",
		"dist-all": "electron-builder -mwl --publish=always"
	},
	"author": "Codiscite",
	"license": "ISC",
	"devDependencies": {
		"@cliqz/adblocker-electron": "^1.1.0",
		"blockstack": "^20.0.0-alpha.4",
		"child_process": "^1.0.2",
		"cors": "^2.8.5",
		"cross-fetch": "^3.0.4",
		"dragula": "^3.7.2",
		"electron": "^4.2.11",
		"electron-builder": "^20.28.4",
		"electron-tabs": "^0.9.5",
		"express": "^4.17.1",
		"favicon-getter": "^1.1.3",
		"gulp": "^3.9.1",
		"jsonfile": "^2.4.0",
		"uuid": "^3.0.1"
	},
	"dependencies": {
		"@cliqz/adblocker-electron": "^1.1.0",
		"blockstack": "^20.0.0-alpha.4",
		"child_process": "^1.0.2",
		"cors": "^2.8.5",
		"cross-fetch": "^3.0.4",
		"dragula": "^3.7.2",
		"electron-tabs": "^0.9.5",
		"express": "^4.17.1",
		"favicon-getter": "^1.1.3",
		"gulp": "^3.9.1",
		"jsonfile": "^2.4.0",
		"uuid": "^3.0.1"
	}
}